{
    "manifest_version": 2,
    "name": "easy-proxy",
    "description": "easy proxy",
    "version": "0.0.1",
    "options_page": "options.html",
    "permissions": [
      "proxy",
      "tabs",
      "unlimitedStorage",
      "storage",
      "<all_urls>",
      "webRequest",
      "webRequestBlocking"
    ]
}